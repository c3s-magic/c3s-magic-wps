[server]
parallelprocesses = 4

url = http://localhost:5000/wps

#url = https://portal.c3s-magic.eu/copernicus-wps/
outputurl = {{ WPS_OUTPUT_URL }}

[logging]
level = DEBUG
file = pywps.log
database = sqlite:///pywps-logs.sqlite
format = %(asctime)s] [%(levelname)s] line=%(lineno)s module=%(module)s %(message)s

[data]
archive_root =/cmip5
obs_root = /obs
